extends layouts/main
include mixins/videoBlock

block content
    p Search
    .search__header
        h3 Searching for #{searchingFor}
    .search__videos
        if videos.length === 0
            h5 No Video found
        each item in videos
            +videoBlock({
              title: item.title,
              views: item.views,
              videoFile: item.videoFile,
              id: item.id
            })